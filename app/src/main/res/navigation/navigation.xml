<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.saliev.cart.Fragment.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_cotegoryFragment2"
            app:destination="@id/cotegoryFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/cotegoryFragment"
        android:name="com.saliev.cart.Fragment.CotegoryFragment"
        android:label="fragment_cotegory"
        tools:layout="@layout/fragment_cotegory" />
    <fragment
        android:id="@+id/fireStoreFragment"
        android:name="com.saliev.cart.Fragment.FireStoreFragment"
        android:label="fragment_fire_store"
        tools:layout="@layout/fragment_fire_store" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.saliev.cart.Fragment.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
</navigation>